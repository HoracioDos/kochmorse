   <t w="17">bar</t> <t w="15">bijelo polje</t> <t w="13">cetinje</t> <t w="13">budva</t>
   <t w="11">berane</t> <t w="10">ulcinj</t> <t w="10">tivat</t> <t w="9">rozaje</t>
