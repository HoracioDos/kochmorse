<?xml version="1.0" ?>

<rules>
 <rule id="call-prefix-s5">
  <t>s5</t>
  <any-number/>
 </rule>

 <rule id="name-s5">
  <one-of>
   <t>franc</t> <t>janez</t> <t>anton</t> <t>ivan</t> <t>jozef</t>
   <t>marija</t> <t>ana</t> <t>maja</t> <t>irena</t> <t>mojca</t>
  </one-of>
 </rule>

 <!-- A list of possible cities for dxcc S5 weighted by their population -->
 <rule id="city-s5">
